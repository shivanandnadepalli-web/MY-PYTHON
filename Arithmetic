age= int(input("enter your age:"))
name= (input("enter your name:"))
weight=float(input("enter your weight:"))
print("age is:",age)
print("name is:",name)
print("weight is:",)
add=age+weight
sub=age-weight
mul=age*weight
div1=age/weight
div2=age//weight
div3=age%weight
print(add,sub,mul,div1,div2,div3)
